  <class name="AgentStatusAlertTime" dynamic-update="true" dynamic-insert="true">
    <id name="Id" column="AgentStatusAlertTimeId">
      <generator class="hilo"/>
    </id>
    <property name="AlertTimeOutSecond" not-null="true"/>
    <property name="AlertTimeOutSecond2" not-null="true"/>
