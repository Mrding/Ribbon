<Window
    x:Class="Luna.GUI.Views.Infrastructure.LaborRuleManageView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:cal="http://www.caliburnproject.org"
    xmlns:inf="http://www.grandsys.com/luna"
    xmlns:uc="clr-namespace:Luna.GUI.UserControls"
    xmlns:themes="http://schemas.actiprosoftware.com/winfx/xaml/themes"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
    xmlns:local="clr-namespace:Luna.GUI.Views.Infrastructure"
    Title="{inf:Resource Administration_BasicLaborRule_WindowTitle}"
    WindowStartupLocation="CenterScreen" mc:Ignorable="d">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Views/Administration/LaborRuleManageViewStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Window.Resources>
    <uc:MasterControl FunctionName="{inf:Resource Administration_BasicLaborRule_Title}"/>
</Window>
