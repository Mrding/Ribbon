<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Luna.Infrastructure.Domain" namespace="Luna.Infrastructure.Domain">
  <class name ="AgentPreference">
    <id name="Id" column="EmployeeId">
      <generator class="foreign">
        <param name="property">Agent</param>
      </generator>
    </id>
    <one-to-one name="Agent" class="Employee" foreign-key="FK_AgentPreference_Employee" constrained="true" />
    <property name="LegalHoliday"/>
    <property name="WeekDayPerfect"/>
    <property name="MonthDayPerfect"/>
    <property name="TimeIntervelPerfect" length="600"/>
  </class>
</hibernate-mapping>